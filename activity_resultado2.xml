<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activityResultado">

    <Button
        android:id="@+id/buttonVoltar"
        android:layout_width="137dp"
        android:layout_height="55dp"
        android:layout_marginStart="123dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="101dp"
        android:text="@string/btnVoltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lblClima2"
        android:layout_width="105dp"
        android:layout_height="36dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="199dp"
        android:text="@string/lblClima"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblTemp2"
        android:layout_width="151dp"
        android:layout_height="33dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="128dp"
        android:text="@string/lblTemperatura"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblClima2" />

    <TextView
        android:id="@+id/tvClima"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:layout_marginTop="199dp"
        android:layout_marginEnd="151dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lblClima2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvTemperatura"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="142dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.717"
        app:layout_constraintStart_toEndOf="@+id/lblTemp2"
        app:layout_constraintTop_toBottomOf="@+id/tvClima" />

</androidx.constraintlayout.widget.ConstraintLayout>